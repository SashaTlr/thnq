<%# <% content_for :header, @entries.id %>	

<head>
	<link href="https://fonts.googleapis.com/css?family=Lato" rel="stylesheet">
</head>
<header>Header</header>

<div class="body">
	<div class="title">	
		<h1>Entries #[1]</h1>
		<button>Add Image</button>
		<button>Submit</button>
	</div>

	<div class="chat">

		<div class="chat-style chat-bot">
			Hello, [username], welcome to thnq! Let's enter your first journal entry.
		</div>
		<div class="chat-style chat-bot">	
			What do you appreciate about [subject]?
		</div>
		<div class="chat-style chat-user">
			<div class="form">
				<form>
					<textarea id="input-box" placeholder="Click to start writing"></textarea>
					<input type="submit" value="Submit">
				</form>
			</div>
		</div>
		

	</div>
</div>
<script type="text/javascript" ></script>
